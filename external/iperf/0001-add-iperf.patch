+    if ( cond ) {                               \
+      warn( msg, __FILE__, __LINE__ );          \
+      thread_stop(settings);                    \
+    }                                           \
+  } while( 0 )
+#else
+#define FAIL( cond, msg, settings )             \
+  do {                                          \
+    if ( cond ) {                               \
+      warn( msg, __FILE__, __LINE__ );          \
+      exit( 1 );                                \
+    }                                           \
+  } while( 0 )
+#endif
+
+#define WARN( cond, msg )                       \
+  do {                                          \
