+/* -------------------------------------------------------------------
+ * Delete memory (hostname strings).
+ * ------------------------------------------------------------------- */
+
+Client::~Client() {
+    if ( mSettings->mSock != INVALID_SOCKET ) {
+        int rc = close( mSettings->mSock );
+        WARN_errno( rc == SOCKET_ERROR, "close" );
+        mSettings->mSock = INVALID_SOCKET;
+    }
+    DELETE_ARRAY( mBuf );
+} // end ~Client
