+        #define SPECIAL_OSF1_EXTERN_C_START extern "C" {
+        #define SPECIAL_OSF1_EXTERN_C_STOP  }
+    #else
+        #define SPECIAL_OSF1_EXTERN_C_START
+        #define SPECIAL_OSF1_EXTERN_C_STOP
+    #endif
+#endif /* HAVE_CONFIG_H */
+
+/* turn off assert debugging */
