+      warn_errno( msg, __FILE__, __LINE__ );    \
+      exit( 1 );                                \
+    }                                           \
+  } while( 0 )
+#endif
+
+#define WARN_errno( cond, msg )                 \
+  do {                                          \
+    if ( cond ) {                               \
+      warn_errno( msg, __FILE__, __LINE__ );    \
+    }                                           \
+  } while( 0 )
