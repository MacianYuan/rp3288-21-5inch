+             gnu_getopt_long( argc, argv, short_options,
+                              long_options, NULL )) != EOF ) {
+        Settings_Interpret( option, gnu_optarg, mSettings );
+    }
+
+    for ( int i = gnu_optind; i < argc; i++ ) {
+        fprintf( stderr, "%s: ignoring extra argument -- %s\n", argv[0], argv[i] );
+    }
