+typedef struct ReportStruct {
+    int packetID;
+    max_size_t packetLen;
+    struct timeval packetTime;
+    struct timeval sentTime;
+    int errwrite;
+    int emptyreport;
+    int socket;
+} ReportStruct;
+
+
+/*
+ * The type field of ReporterData is a bitmask
+ * with one or more of the following
+ */
+#define    TRANSFER_REPORT      0x00000001
+#define    SERVER_RELAY_REPORT  0x00000002
+#define    SETTINGS_REPORT      0x00000004
+#define    CONNECTION_REPORT    0x00000008
+#define    MULTIPLE_REPORT      0x00000010
+
+typedef union {
+    ReadStats read;
+    WriteStats write;
+} TCPStats;
+
+typedef struct Transfer_Info {
+    void *reserved_delay;
+    int transferID;
+    int groupID;
+    int cntError;
+    int cntOutofOrder;
+    int cntDatagrams;
+    int IPGcnt;
+    int socket;
+    TransitStats transit;
+    TCPStats tcp;
+    // Hopefully int64_t's
+    max_size_t TotalLen;
+    double jitter;
+    double startTime;
+    double endTime;
+    double IPGsum;
+    // chars
+    char   mFormat;                 // -f
+    char   mEnhanced;               // -e
