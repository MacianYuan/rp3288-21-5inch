&pwm0 {
        status = "okay";
};

&lvds {
	status = "okay";
        ports {
                lvds_out: port@1 {
                        reg = <1>;
                        #address-cells = <1>;
                        #size-cells = <0>;

                        lvds_out_panel: endpoint@0 {
                                reg = <0>;
                                remote-endpoint = <&panel_in_lvds>;
                        };
                };
        };
};

&lvds_panel {
        status = "okay";
        compatible ="simple-panel";
        backlight = <&backlight>;
        bus-format = <MEDIA_BUS_FMT_RGB888_1X24>;
        enable-gpios = <&gpio7 2 GPIO_ACTIVE_HIGH>;
        prepare-delay-ms = <100>;
	enable-delay-ms = <100>;
	pinctrl-0 = <&lcd_en>;
        rockchip,data-mapping = "jeida";		//""jeida"//vesa
        rockchip,data-width = <24>;	//<18>
        rockchip,output = "duallvds";

        display-timings {
                native-mode = <&timing0>;
                timing0: timing0 {
                        clock-frequency = <140000000>;
                        hactive = <1920>;
                        vactive = <1080>;
                        hback-porch = <100>;
                        hfront-porch = <80>;
                        vback-porch = <20>;
                        vfront-porch = <20>;
                        hsync-len = <20>;
                        vsync-len = <10>;
                        hsync-active = <0>;
                        vsync-active = <0>;
                        de-active = <0>;
                        pixelclk-active = <0>;
                };
        };
};
&route_lvds {
	status = "okay";
};

&pinctrl {
	lcd {
		lcd_en: lcd-en  {
			rockchip,pins = <7 2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};
&edp_in_vopl {
        status = "disabled";
};
&dsi0_in_vopl {
        status = "disabled";
};
&dsi1_in_vopl {
        status = "disabled";
};
